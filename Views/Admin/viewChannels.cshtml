@{
    ViewBag.Title = "Cartoon Hub";
}

@using cartoon.Models

<div class="container" id="sheet" style="width:80%;">
    <h2>All Users Details</h2>

    <table class="table table-bordered table-hover" style="margin-left:-4%;">
        <thead style="font-weight:700">
            <tr>

                <th>Id</th>
                <th>Name</th>
                

            </tr>
        </thead>
        <tbody>
            @{
                foreach (channel u in Model)
                {
                    <tr>
                        <td>@u.Id</td>
                        <td>@u.name</td>
                        

                        <td>
                            <button class="form-control" onclick="self.location='/Admin/deletechannel/@u.Id'">Delete</button>

                        </td>

                    </tr>

                }
            }
        </tbody>
    </table>

</div>

